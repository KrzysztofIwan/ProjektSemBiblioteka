<Window x:Class="ProjektSemBiblioteka.Views.Zwrot_literaturyDetails"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ProjektSemBiblioteka.Views"
        mc:Ignorable="d"
        Title="Zwrot_literaturyDetails" Height="450" Width="800">
    <Grid>
        <ListView ItemsSource="{Binding ZwrotList}">
            <ListView.View>
                <GridView>
                    <GridView.Columns>
                        <GridViewColumn Header="Id_Uzytkownika" DisplayMemberBinding="{Binding Id_Uzytkownika}"/>
                        <GridViewColumn Header="Id_Literatury" DisplayMemberBinding="{Binding Id_Literatury}"/>
                        <GridViewColumn Header="Data_Zwrotu" DisplayMemberBinding="{Binding Data_Zwrotu}"/>
                    </GridView.Columns>
                </GridView>
            </ListView.View>
        </ListView>
    </Grid>
</Window>
